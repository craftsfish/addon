{
  "description": "Automatically handle orders of JD",
  "manifest_version": 2,
  "name": "JD",
  "version": "1.0",
  "icons": {
    "48": "icons/beasts-48.png"
  },

  "permissions": [
    "https://sz.jd.com/realTime/realTop.html",
    "https://sz.jd.com/realTime/*",
    "https://item.jd.com/*",
    "http://www.dasbu.com/seller",
    "http://www.dasbu.com/seller/renwu/create?step=1",
    "activeTab",
    "tabs",
    "alarms"
  ],  

  "background": {
    "scripts": ["background-script.js"]
  },

  "browser_action": {
    "default_icon": "icons/beasts-32.png",
    "default_title": "JD"
  }
}
