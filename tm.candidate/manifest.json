{
  "description": "Retrieve Candidate Criterias of TMall Item",
  "manifest_version": 2,
  "name": "TMall Criterias",
  "version": "1.0",
  "icons": {
    "48": "icons/beasts-48.png"
  },

  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs",
    "webNavigation",
    "webRequest",
    "alarms"
  ],

  "browser_action": {
    "default_icon": "icons/beasts-32.png",
    "default_title": "TMallKeyword"
  },

  "background": {
    "scripts": ["common.js", "background-script.js"]
  }
}
